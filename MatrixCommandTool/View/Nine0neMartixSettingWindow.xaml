                    <RowDefinition Height="*"/>
                    <RowDefinition Height="60"/>
                </Grid.RowDefinitions>
                <ListBox
                    
                            Margin="0,4"
                            Background="Transparent"
                            BorderThickness="0"
                    HorizontalAlignment="Center"
                            ItemsSource="{Binding OutPutColorChannels}"
                            ScrollViewer.VerticalScrollBarVisibility="Disabled" SelectionMode="Multiple">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel />
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="{x:Type ListBoxItem}">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type ListBoxItem}">

                                        <StackPanel
                                                    Orientation="Horizontal"
                                                    x:Name="bd"
                                                    Height="48"
                                                    Background="Transparent"
                                                    Cursor="Hand">
                                            <CheckBox   IsChecked="{Binding IsCheckChannel}"
                                                                x:Name="selectCommand"
                                                                Width="40"
                                                                Height="40"
                                                                VerticalAlignment="Center"
                                                                HorizontalAlignment="Center"
                                                                Cursor="Hand"
                                                                Foreground="#C0C4CC"
                                                                Style="{StaticResource CheckBoxDefaultStyle}"/>
                                            <TextBlock
                                                                x:Name="txt_name"

                                                                HorizontalAlignment="Left"
                                                                VerticalAlignment="Center"
                                                                FontSize="16"
                                                                Foreground="#FFFFFF"
                                                                Text="{Binding OnputChannel}" />
                                        </StackPanel>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>
                <Border Margin="12" Grid.Column="1" Background="Transparent"   Height="220">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>

                        <StackPanel
                Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="设置色调" 
                               Width="60"
                               Foreground="#FFFFFF"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding ColorSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>

                        <StackPanel
                    Grid.Row="1"
                    Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="对比度" 
                               Width="60"
                               Height="24"
                               FontSize="14"
                               Foreground="#FFFFFF"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding ContractSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>

                        <StackPanel
                    Grid.Row="2"
                    Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="饱和度" 
                               Width="60"
                               Foreground="#FFFFFF"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding SaturationSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>

                        </StackPanel>
                        <StackPanel
                    Grid.Row="3"
                    Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="设置R值" 
                               Foreground="#FFFFFF"
                               Width="60"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding RSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>
                    </Grid>

                </Border>

                <Border  Margin="12" Grid.Column="2"   Height="200">
                    <Grid Margin="0,0,30,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel
                Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="亮度" 
                               Width="60"
                               Foreground="#FFFFFF"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding LightSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>

                        <StackPanel
                    Grid.Row="1"
                    Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="设置G值" 
                               Foreground="#FFFFFF"
                               Width="60"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding GSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>

                        <StackPanel
                    Grid.Row="2"
                    Margin="12"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                            <TextBlock Text="设置B值" 
                               Foreground="#FFFFFF"
                               Width="60"
                               Height="24"
                               FontSize="14"
                               TextAlignment="Center"/>
                            <controls:NumberControl Width="220" Height="30"
                                                    VerticalContentAlignment="Center"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    HorizontalContentAlignment="Center"
                                                    Number="{Binding BSetValue,Mode=TwoWay}"
                                                    MaxNumber="99"
                                                    MinNumber="0"/>
                        </StackPanel>


                    </Grid>

                </Border>

                <StackPanel Grid.Row="1" Grid.ColumnSpan="3" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top">
                    <Button Width="120"  Grid.Row="1"  Height="40" Content="获取色彩参数" HorizontalAlignment="Left" VerticalAlignment="Center" Command="{Binding GetColorInfoCommand}" Style="{DynamicResource ButtonStyle}"/>
                    <Button Margin="120 0 0 0" Width="120"  Grid.Row="1" Grid.Column="1"  Height="40" Content="复位色彩参数" HorizontalAlignment="Left" VerticalAlignment="Center" Command="{Binding ResetColorInfoCommand}" Style="{DynamicResource ButtonStyle}"/>
                    <TextBlock Margin="120 0 0 0"  Grid.Row="1" Grid.Column="1" Foreground="Black" FontSize="16" Text="{Binding Tips,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                </StackPanel>

                <ListBox    Grid.Column="3"
                            Grid.Row="0"
                            Grid.RowSpan="2"
                            Margin="0 12"
                            Background="Transparent"
                            BorderThickness="0"
                            HorizontalAlignment="Center"
                            ItemsSource="{Binding ColorSettingsList}"
                            ScrollViewer.VerticalScrollBarVisibility="Disabled">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel />
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="{x:Type ListBoxItem}">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                        <StackPanel
                                                    Orientation="Horizontal"
                                                    x:Name="bd"
                                                    Background="Transparent"
                                                    Cursor="Hand">
                                            <TextBlock
                                                                x:Name="txt_name"
                                                                Margin="5,0,0,0"
                                                                HorizontalAlignment="Left"
                                                                VerticalAlignment="Center"
                                                                FontSize="16"
                                                                Foreground="#FFFFFF"
                                                                Text="{Binding ColorSettings}" />
                                        </StackPanel>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>
            </Grid>

        </Border>


        <!--加载场景动画-->
        <Border
            Grid.RowSpan="4"
            Grid.ColumnSpan="2"
            CornerRadius="4"
            Visibility="{Binding IsOverAllMsg}">
            <Border.Background>
                <SolidColorBrush Opacity=".4" Color="#000000" />
            </Border.Background>
            <!--   -->
            <Grid Width="280">
                <Grid x:Name="ro1"  VerticalAlignment="Center" HorizontalAlignment="Center" Width="200" Height="200"  RenderTransformOrigin="0.5,0.5">
                    <myLib:CirclePointRingLoading Width="100" Height="100" IsActive="True" IsLarge="False" Foreground="#3ca9fe"/>
                </Grid>
            </Grid>
        </Border>
    </Grid>
</Window>

