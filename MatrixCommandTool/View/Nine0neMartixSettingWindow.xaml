<Window x:Class="MatrixCommandTool.View.Nine0neMartixSettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MatrixCommandTool.View"
         xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:controls="clr-namespace:MatrixCommandTool.Controls"
          xmlns:myLib="clr-namespace:MyLoadingLib;assembly=MyLoadingLib"
        mc:Ignorable="d"
        Title="SetCommandWindow" Height="850" Width="1400"  Style="{StaticResource NoneWindowStyle_Radius}" WindowStartupLocation="CenterScreen"
        DataContext="{Binding Source={StaticResource Locator}, Path=NineOneMartixSettingVM}">
    <Grid x:Name="nineonegrid" Background="{DynamicResource BgDefaultColor}">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Width="880"  
                   HorizontalAlignment="Left" 
                   Fill="Transparent"
                   Grid.ColumnSpan="2"
                   MouseMove="DragWindowMove"/>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="40" HorizontalAlignment="Right" Margin="10" Grid.Column="1">
            <controls:TitleSwitchButtonControl
                x:Name="switch"
                Width="50"
                Margin="0,0,16,0"
                VerticalAlignment="Center"
                Background="{DynamicResource SliderDefaultColor}"
                CheckedText="蜂鸣器开"
                DockPanel.Dock="Right"
                Foreground="#FFFFFF"
                Style="{StaticResource VerticalTitleSwitchButtonStyle}"
                UnCheckedText="蜂鸣器关" 
                IsChecked="{Binding IsOpenBuzzer}"
                Command="{Binding SwitchBuzzerCommand}" Cursor="Hand"/>

            <Button x:Name="changeskin" Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}" Width="30" Height="30" Padding="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFFFF" Click="changeskin_Click">
                <Button.ContextMenu>
                    <ContextMenu>
                        <MenuItem x:Name="menuBlue"  Header="夏日初恋" Click="menuAero_Click"/>
                    </ContextMenu>
                </Button.ContextMenu>
                <materialDesign:PackIcon Kind="Color" ToolTip="主题"/>
            </Button>
            <Button Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}" Width="30" Height="30" Padding="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFFFF" Click="WinMin_Click" Cursor="Hand">
                <materialDesign:PackIcon Kind="Minimize"/>
            </Button>
            <Button Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}" Width="30" Height="30" Padding="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFFFF" Click="WindowMax_Click" Cursor="Hand">
                <materialDesign:PackIcon Kind="Maximize"/>
            </Button>
            <Button x:Name="ButtonClose" Style="{StaticResource MaterialDesignFloatingActionMiniAccentButton}" Width="30" Height="30" Padding="0" Background="{x:Null}" Foreground="#FFFFFF" BorderBrush="{x:Null}" Click="ButtonClose_Click" Cursor="Hand">
                <materialDesign:PackIcon Kind="Close"/>
            </Button>
        </StackPanel>
        <Grid VerticalAlignment="Top" Grid.Row="1" Grid.ColumnSpan="2" Margin="12 0 ">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <RadioButton
                            x:Name="rbtn_switch"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Top"
                            GroupName="Tb"
                            Grid.Column="0"
                            IsChecked="{Binding IsCheckedSwitch}"
                            Content="输入输出切换"
                            Height="50"
                             Style="{DynamicResource RadioButtonStyle}"/>
            
            <RadioButton
                            x:Name="rbtn_color"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Top"
                            GroupName="Tb"
                            Grid.Column="3"
                            IsChecked="{Binding IsCheckedColor}"
                            Content="指令发送"
                            Height="50" 
                            Style="{DynamicResource RadioButtonStyle}"/>

            <RadioButton
                            x:Name="rbtn_EDID"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Top"
                            GroupName="Tb"
                            IsChecked="{Binding IsCheckedEDID}"
                            Content="EDID学习"
                            Height="50"
                            Style="{DynamicResource RadioButtonStyle}"
                            Grid.Column="2"/>

            <RadioButton
                            x:Name="rbtn_signal"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Top"
                            GroupName="Tb"
                            IsChecked="{Binding IsCheckedSignal}"
                            Content="信号设置"
                            Height="50"
                            Grid.Column="1"
                            Style="{DynamicResource RadioButtonStyle}"/>
            <RadioButton
                            x:Name="rbtn_seeting"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Top"
                            GroupName="Tb"
                            IsChecked="{Binding IsCheckedSetting}"
                            Content="系统设置"
                            Height="50"
                            Style="{DynamicResource RadioButtonStyle}"
                            Grid.Column="4"/>
        </Grid>
        <Border Grid.Row="2"  Margin="12" Grid.ColumnSpan="2" >
            <TabControl
                    Margin="2,0,0,2"
                    Background="Transparent"
                    BorderThickness="1"
                BorderBrush="#ffffff">
                <TabItem Header="SwitchInput"
                         IsSelected="{Binding ElementName=rbtn_switch,Path=IsChecked}"
                         Visibility="Collapsed">
                    <Grid Width="1350">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1.5*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>

                        <Border BorderBrush="#FFFFFF" BorderThickness="1" Margin="12 0" >
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="60"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Label Content="输入1" Foreground="#FFFFFF" FontSize="18" Grid.Column="1" VerticalContentAlignment="Center"/>
                                <Label Content="输入2" Foreground="#FFFFFF" FontSize="18" Grid.Column="2" VerticalContentAlignment="Center"/>
                                <Label Content="输入3" Foreground="#FFFFFF" FontSize="18" Grid.Column="3" VerticalContentAlignment="Center"/>
                                <Label Content="输入4" Foreground="#FFFFFF" FontSize="18" Grid.Column="4" VerticalContentAlignment="Center"/>

                                <Label Content="输出1" Foreground="#FFFFFF" FontSize="18" Grid.Row="1" VerticalContentAlignment="Center"/>
                                <Label Content="输出2" Foreground="#FFFFFF" FontSize="18" Grid.Row="2" VerticalContentAlignment="Center"/>
                                <Label Content="输出3" Foreground="#FFFFFF" FontSize="18" Grid.Row="3" VerticalContentAlignment="Center"/>
                                <Label Content="输出4" Foreground="#FFFFFF" FontSize="18" Grid.Row="4" VerticalContentAlignment="Center"/>
                                <ListBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Grid.ColumnSpan="4"
                                    Grid.RowSpan="4"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    ItemsSource="{Binding ChannelsList}"
                                    ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                    ScrollViewer.CanContentScroll="True">
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <UniformGrid Rows="4" Columns="4" />
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                    <ListBox.ItemContainerStyle>
                                        <Style TargetType="{x:Type ListBoxItem}">
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                                        <StackPanel
                                                    Orientation="Horizontal"
                                                    x:Name="bd"
                                                    Height="48"
                                                    Background="Transparent"
                                                    Cursor="Hand">
                                                            <Button Style="{DynamicResource ButtonStyle}"
                                                                x:Name="button"
                                                                Background="#009FD3"
                                                                Width="20" Height="20" HorizontalAlignment="Center"  Margin="10"
                                                                Command="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.SwitchOneCommand}"
                                                                CommandParameter="{Binding .}" 
                                                                Cursor="Hand"/>
                                                        </StackPanel>
                                                        <ControlTemplate.Triggers>
                                                            <DataTrigger Binding="{Binding IsCheckChannel}" Value="True">
                                                                <Setter Property="Background" Value="#16FBF8" TargetName="button"/>
                                                            </DataTrigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </ListBox.ItemContainerStyle>
                                </ListBox>

                                <Button Style="{DynamicResource ButtonStyle}"
                                        Background="#009FD3"
                                        Width="120" Height="40" HorizontalAlignment="Center" Margin="10 0 0 0"
                                        Command="{Binding QuerySwitchChannelCommand}"
                                        x:Name="outputfour"
                                        Content="获取通道状态"
                                        Grid.Row="5"
                                        Grid.Column="3"
                                        Grid.ColumnSpan="2"/>
                            </Grid>

                        </Border>

                        <Border BorderBrush="#FFFFFF" BorderThickness="1" Margin="12 0" Visibility="Collapsed">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <DataGrid Background="Transparent" 
                                  HorizontalAlignment="Center"
                                  CanUserAddRows="False" 
                                  CanUserDeleteRows="False" 
                                  CanUserReorderColumns="False" 
                                  CanUserResizeRows="False"
                                  CanUserSortColumns="False" 
                                  AutoGenerateColumns="False" 
                                  SelectionMode="Single"
                                  BorderThickness="1"  
                                  RowHeaderWidth="0" 
                                  ItemsSource="{Binding PagesSource}" GridLinesVisibility="None">
                                    <DataGrid.ColumnHeaderStyle>
                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                            <Setter Property="Padding" Value="18 0 0 0" />
                                            <Setter Property="VerticalContentAlignment" Value="Center" />
                                            <Setter Property="Foreground" Value="White" />
                                            <Setter Property="Background" Value="Transparent" />
                                            <Setter Property="FontSize" Value="16" />
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type DataGridColumnHeader}">
                                                        <Border BorderBrush="Transparent"
                                                        Margin="0"
                                                        BorderThickness="1 1 1 1"
                                                        Height="56"
                                                        Background="{TemplateBinding Background}"
                                                        Padding="{TemplateBinding Padding}">
                                                            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition />
                                                                    <ColumnDefinition Width="Auto" />
                                                                </Grid.ColumnDefinitions>
                                                                <ContentPresenter RecognizesAccessKey="True" VerticalAlignment="Center" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                                            </Grid>
                                                        </Border>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </DataGrid.ColumnHeaderStyle>
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="输入设备" IsReadOnly="True" Width="100" Binding="{Binding OutputName}"/>
                                        <DataGridTemplateColumn Header="输出1" Width="80">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Style="{DynamicResource ButtonStyle}"
                                                    Width="20" Height="20" HorizontalAlignment="Center" Margin="10 0 0 0"
                                                    Command="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.SwitchOneCommand}"
                                                    CommandParameter="{Binding OutputName}" 
                                                    Cursor="Hand"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="输出2" Width="80">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Style="{DynamicResource ButtonStyle}"
                                            Width="20" Height="20" HorizontalAlignment="Center" Margin="10 0 0 0"
                                            Command="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.SwitchTwoCommand}"
                                            CommandParameter="{Binding OutputName}" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="输出3" Width="80">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Style="{DynamicResource ButtonStyle}"
                                            Width="20" Height="20" HorizontalAlignment="Center" Margin="10 0 0 0"
                                            Command="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.SwitchThreeCommand}"
                                            CommandParameter="{Binding OutputName}" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn Header="输出4" Width="80">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button Style="{DynamicResource ButtonStyle}"
                                                            Background="#009FD3"
                                            Width="20" Height="20" HorizontalAlignment="Center" Margin="10 0 0 0"
                                            Command="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.SwitchFourCommand}"
                                            CommandParameter="{Binding OutputName}" 
                                                            x:Name="outputfour"/>
                                                    <!--<DataTemplate.Triggers>
                                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=local:Nine0neMartixSettingWindow,Mode=FindAncestor},Path=DataContext.IsAlreadySetOuputFour}" Value="False">
                                                            <Setter TargetName="outputfour" Property="Background"  Value="#16FBF8"/>
                                                        </DataTrigger>
                                                    
                                                    </DataTemplate.Triggers>-->
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                    </DataGrid.Columns>
                                    <DataGrid.CellStyle>
                                        <Style TargetType="{x:Type DataGridCell}">
                                            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
                                            <Setter Property="FontSize" Value="16"/>
                                            <Setter Property="HorizontalAlignment" Value="Center"/>
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type DataGridCell}">
                                                        <Border Padding="10" >
                                                            <ContentPresenter/>
                                                        </Border>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                            <Style.Triggers>
                                                <MultiTrigger>
                                                    <!-- 失去焦点时背景颜色 -->
                                                    <MultiTrigger.Conditions>
                                                        <Condition Property="IsSelected" Value="true"/>
                                                        <Condition Property="Selector.IsSelectionActive" Value="false"/>
                                                    </MultiTrigger.Conditions>
                                                    <Setter Property="Foreground" Value="White"/>

                                                </MultiTrigger>

                                            </Style.Triggers>
                                        </Style>
                                    </DataGrid.CellStyle>
                                    <DataGrid.RowStyle>
                                        <Style TargetType="DataGridRow">
                                            <Setter Property="Background" Value="Transparent"/>

                                        </Style>
                                    </DataGrid.RowStyle>
                                </DataGrid>
                                <TextBlock Grid.Row="1" Text="*一个输出只能给一个输入" Foreground="#FFFFFF" FontSize="20" HorizontalAlignment="Center"/>
                            </Grid>
                        </Border>

                        <Border  HorizontalAlignment="Left" Grid.Column="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <Border  BorderBrush="#FFFFFF" BorderThickness="1">
                                    <Grid Margin="12">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Button 
                        Width="240"
                        Height="50"
                        Content="查询所有预案"
                        Command="{Binding AllPresetScanCommand}"
                         Style="{DynamicResource ButtonStyle}"/>
                                        <ListBox
                                x:Name="input"
                                Grid.Row="1"
                                Margin="8,0,0,0"
                                Background="Transparent"
                                BorderThickness="0"
                                ItemTemplate="{StaticResource BoardItemTemplate}"
                                ItemsSource="{Binding ScenePlanList}"
                                ScrollViewer.CanContentScroll="True"
                                SelectedItem="{Binding SelectedScenePlan}">
                                            <ListBox.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <WrapPanel Orientation="Horizontal" Margin="5 0 0 0"/>
                                                </ItemsPanelTemplate>
                                            </ListBox.ItemsPanel>
                                            <ListBox.ItemContainerStyle>
                                                <Style TargetType="{x:Type ListBoxItem}">
                                                    <Setter Property="Template">
                                                        <Setter.Value>
                                                            <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                                                <StackPanel
                                                    Orientation="Horizontal"
                                                    x:Name="bd"
                                                    Background="Transparent"
                                                    Cursor="Hand">
                                                                    <TextBlock
                                                                x:Name="txt_name"
                                                                Margin="5,0,0,0"
                                                                HorizontalAlignment="Left"
                                                                VerticalAlignment="Center"
                                                                FontSize="16"
                                                                Foreground="#FFFFFF"
                                                                Text="{Binding PlanInfo}" />
                                                                </StackPanel>
                                                            </ControlTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </Style>
                                            </ListBox.ItemContainerStyle>
                                        </ListBox>
                                    </Grid>
                                </Border>

                                <Border Grid.Column="1"  BorderBrush="#FFFFFF" BorderThickness="1">
                                    <Grid Margin="12">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="50"/>
                                            <RowDefinition Height="20"/>
                                            <RowDefinition Height="80"/>
                                            <RowDefinition Height="80"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.ColumnSpan="4" Orientation="Horizontal" >
                                            <TextBlock Text="修改" FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            <TextBox Text="{Binding ModifyPlanIndex}" FontSize="14" Foreground="Black" Background="#ffffff" Width="60" Height="30" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            <TextBlock Text="号预案名称为" FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            <TextBox Text="{Binding NewPlanName}" FontSize="16" Foreground="Black" Background="#ffffff" Width="150" Height="30"/>
                                        </StackPanel>

                                        <TextBlock Grid.Row="1"  VerticalAlignment="Top" Grid.Column="2" Grid.ColumnSpan="3" Text="*修改的场景名称长度不超过10"  FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Left"/>

                                        <StackPanel Grid.Row="2" Orientation="Horizontal" Grid.ColumnSpan="4"  >
                                            <TextBlock Text="选择" FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            <TextBox TextAlignment="Center" Text="{Binding PlanIndex,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" FontSize="16" Foreground="Black" Background="#ffffff" Width="60" Height="30"/>
                                            <TextBlock Text="号预案" FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>

                                        </StackPanel>
                                        <!--<StackPanel Grid.Row="3" Orientation="Horizontal" Grid.ColumnSpan="4"  >
                                        <TextBlock Text="保存场景预案到新增序号" FontSize="16" Foreground="Black" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    </StackPanel>-->

                                        <StackPanel Grid.Row="3" Orientation="Horizontal" Grid.ColumnSpan="4" >
                                            <TextBlock Text="保存预案到序号" FontSize="16" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            <TextBox Text="{Binding SavePlanToIndex}" TextAlignment="Center" FontSize="16" Foreground="Black" Background="#ffffff" Width="60" Height="30"/>


                                        </StackPanel>
                                        <Button Width="60"  Grid.Column="4"   Height="20" Content="确定修改" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding ModifyPlanNameCommand}" Style="{DynamicResource ButtonStyle}"/>
                                        <Button Width="60"  Grid.Row="2" Grid.Column="4"  Height="20" Content="确定选择" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding ChoosePlanCommand}" Style="{DynamicResource ButtonStyle}"/>
                                        <!--<Button Width="60" Grid.Row="3" Grid.Column="4"   Height="20" Content="确定新增" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding SendColorAdjInstructionsCommand}" Style="{DynamicResource ButtonStyle}"/>-->
                                        <Button Width="60" Grid.Row="3" Grid.Column="4"  Height="20" Content="确定保存" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding SavePlanAsIndexCommand}" Style="{DynamicResource ButtonStyle}"/>
                                    </Grid>

                                </Border>
                            </Grid>
                        </Border>
                    </Grid>

                </TabItem>
                
                <TabItem Header="SignalSet"
                         IsSelected="{Binding ElementName=rbtn_signal,Path=IsChecked}"
                         Visibility="Collapsed">
                    <Border BorderBrush="#FFFFFF" BorderThickness="2"    HorizontalAlignment="Center" VerticalAlignment="Top" >
                        <Grid  HorizontalAlignment="Left" Width="1350">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="1.5*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>
                            <Grid Margin="80,0 0 0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <Button Content="获取输入板卡" Command="{Binding QueryAudioCommand}" Margin="0 6 0 0" Style="{DynamicResource ButtonStyle}" Grid.ColumnSpan="5"  Foreground="#ffffff" FontSize="16" Width="220" Height="40" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="输入通道" FontSize="14" Foreground="#ffffff" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <!--<TextBlock Text="输入格式" FontSize="14" Foreground="#ffffff" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
                                <TextBlock Text="音频" FontSize="14" Foreground="#ffffff" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="输入1" FontSize="14" Foreground="#ffffff" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <!--<TextBlock Text="4K60" FontSize="14" Foreground="#FFFFFF" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
                                <!--音频-->
                                <ComboBox
                                    Width="200" Height="40" 
                                    Background="#ffffff"
                                    Foreground="Black"
                                    FontSize="16"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    SelectedIndex="0"                                
                                    ItemsSource="{Binding AudioList}"
                                    SelectedItem="{Binding SelectedOneInAudioName}"
                                    Grid.Row="2"
                                    Grid.Column="2"
                                    Tag="1"
                                    DropDownClosed="ComboBox_DropDownClosed"/>
                                <ComboBox
                                    Width="200" Height="40" 
                                    Background="#ffffff"
                                    Foreground="Black"
                                    FontSize="16"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    SelectedIndex="0"                                
                                    ItemsSource="{Binding AudioList}"
                                    SelectedItem="{Binding SelectedTwoInAudioName}"
                                    Grid.Row="3"
                                    Grid.Column="2"
                                    Tag="2"
                                    DropDownClosed="ComboBox_DropDownClosed"/>
                                <ComboBox
                                    Width="200" Height="40" 
                                    Background="#ffffff"
                                    Foreground="Black"
                                    FontSize="16"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    SelectedIndex="0"                                
                                    ItemsSource="{Binding AudioList}"
                                    SelectedItem="{Binding SelectedInThreeAudioName}"
                                    Grid.Row="4"
                                    Tag="3"
                                    Grid.Column="2"
                                     DropDownClosed="ComboBox_DropDownClosed"/>
                                <ComboBox
                                    Width="200" Height="40" 
                                    Background="#ffffff"
                                    Foreground="Black"
                                    FontSize="16"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    SelectedIndex="0"                                
                                    ItemsSource="{Binding AudioList}"
                                    SelectedItem="{Binding SelectedInFourAudioName}"
                                    DropDownClosed="ComboBox_DropDownClosed"
                                    Grid.Row="5"
                                    Tag="4"
                                    Grid.Column="2"/>
                                <!--<TextBlock Text="4K60" FontSize="14" Foreground="#FFFFFF" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="4K60" FontSize="14" Foreground="#FFFFFF" Grid.Row="4" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="4K60" FontSize="14" Foreground="#FFFFFF" Grid.Row="5" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>-->
                                <TextBlock Text="输入2" FontSize="14" Foreground="#ffffff" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="输入3" FontSize="14" Foreground="#ffffff" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <TextBlock Text="输入4" FontSize="14" Foreground="#ffffff" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>


